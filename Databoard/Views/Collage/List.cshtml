@{
}
@model List<Databoard.Models.Entities.School>

 @if(Model is null)
{
    <p>Empty Record</p>

}
else
{
<table class="table">
 <thead>
     <tr>
         <th>Id</th>
         <th>Medium</th>
         <th>Establish</th>
         <th>Address</th>
         <th>Authorised</th>
         <th>Edit</th>
     </tr>
 </thead>
 <tbody>
     @foreach (var s in Model)
            {
                <tr>
                 <td>@s.Id</td>
                 <td>@s.Name</td>
                 <td>@s.Medium</td>
                 <td>@s.Establish</td>
                 <td>@s.Address</td>
                 <td>@s.Authorised</td>
                 <td>
                  <a class="btn btn-primary" asp-controller="Collage" asp-action="Edit" asp-route-id="@s.Id"> Edit</a>
                 </td>
                </tr>
            }
        </tbody>

 </table>
}
